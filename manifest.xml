<package>
  <description brief="orcdchomp">
     orcdchomp is an implementation of chomp using libcd.
  </description>
  <author>Chris Dellinpr account</author>
  <depend package="openrave"/>
  <export>
    <python path="${prefix}/pythonsrc"/>
    <!--<env var="OPENRAVE_PLUGINS" append=":${prefix}/lib"/>-->
